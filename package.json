{
    "name": "hibernate-log-analyser",
    "displayName": "Hibernate Log analyser",
    "description": "Extract SQL from a hibernate log and renitegrate bind values",
    "version": "1.0.2",
    "homepage": "https://github.com/GreenRover/hibernate-log-analyser",
    "repository": {
        "type": "git",
        "url": "https://github.com/GreenRover/hibernate-log-analyser.git"
    },
    "bugs": {
        "url": "https://github.com/GreenRover/hibernate-log-analyser/issues"
    },
    "license": "MIT",
    "publisher": "greenrover",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "Hibernate",
        "log4j",
        "sql"
    ],
    "activationEvents": [
        "onCommand:extension.extractHibernateLog",
        "onCommand:extension.reloadFile",
        "onCommand:extension.clearFile"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.extractHibernateLog",
                "title": "Extract SQL from Hibernate log",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.reloadFile",
                "title": "Reload current editor",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.clearFile",
                "title": "Clear file in current editor",
                "when": "editorTextFocus"
            }
        ],
        "keybindings": [
            {
                "command": "extension.extractHibernateLog",
                "key": "alt+h",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.reloadFile",
                "key": "alt+f5",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.clearFile",
                "key": "alt+c",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "rxjs": "^5.0.0-beta.12"
    }
}
