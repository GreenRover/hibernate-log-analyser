{
    "name": "hibernate-log-analyser",
    "displayName": "Hibernate Log analyser",
    "description": "Extract SQL from a hibernate log and renitegrate bind values",
    "version": "1.2.6",
    "homepage": "https://github.com/GreenRover/hibernate-log-analyser",
    "repository": {
        "type": "git",
        "url": "https://github.com/GreenRover/hibernate-log-analyser.git"
    },
    "bugs": {
        "url": "https://github.com/GreenRover/hibernate-log-analyser/issues"
    },
    "license": "MIT",
    "publisher": "greenrover",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "Hibernate",
        "log4j",
        "sql"
    ],
    "activationEvents": [
        "onCommand:hibernateLogExtract.extractHibernateLog",
        "onCommand:hibernateLogExtract.reloadFile",
        "onCommand:hibernateLogExtract.clearFile"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Hibernate log Extractor configuration",
            "properties": {
                "hibernateLogExtract.sqlComment": {
                    "type": "boolean",
                    "default": false,
                    "description": "Extract comments from SQL Querys, generated by: hibernate.use_sql_comments. See https://www.thoughts-on-java.org/hibernate-tips-query-comments/"
                },
                "hibernateLogExtract.hql": {
                    "type": "boolean",
                    "default": false,
                    "description": "Extract hql logged over each sql. log4j.logger.org.hibernate=info"
                },
                "hibernateLogExtract.statistic": {
                    "type": "boolean",
                    "default": false,
                    "description": "Extract statistics generated by: log4j.logger.org.hibernate.stat.internal.ConcurrentStatisticsImpl=trace and hibernate.generate_statistics. See https://www.thoughts-on-java.org/how-to-activate-hibernate-statistics-to-analyze-performance-issues/"
                }
            }
        },
        "commands": [
            {
                "command": "hibernateLogExtract.extractHibernateLog",
                "title": "Extract SQL from Hibernate log",
                "when": "editorTextFocus"
            },
            {
                "command": "hibernateLogExtract.reloadFile",
                "title": "Reload current editor",
                "when": "editorTextFocus"
            },
            {
                "command": "hibernateLogExtract.clearFile",
                "title": "Clear file in current editor",
                "when": "editorTextFocus"
            }
        ],
        "keybindings": [
            {
                "command": "hibernateLogExtract.extractHibernateLog",
                "key": "alt+h",
                "when": "editorTextFocus"
            },
            {
                "command": "hibernateLogExtract.reloadFile",
                "key": "alt+f5",
                "when": "editorTextFocus"
            },
            {
                "command": "hibernateLogExtract.clearFile",
                "key": "alt+c",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "rxjs": "^5.0.0-beta.12"
    }
}
