{
    "name": "hibernate-log-analyser",
    "displayName": "Hibernate Log analyser",
    "description": "Extract SQL from a hibernate log and renitegrate bind values",
    "version": "0.0.1",
    "publisher": "greenrover",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.extractHibernateLog"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.extractHibernateLog",
                "title": "Extract SQL from Hibernate log",
                "when": "editorTextFocus"
            }
        ],
        "keybindings": [
            {
                "command": "extension.extractHibernateLog",
                "key": "alt+c e",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
    }
}
